{
    "name": "radebatz/acache",
    "description": "Another cache library.",
    "license": [
        "MIT"
    ],
    "homepage": "http://radebatz.net/mano/",
    "authors": [
        {
            "name": "Martin Rademacher",
            "email": "mano@radebatz.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "provide": {
        "psr/cache-implementation": "1.0.0"
    },
    "support": {
        "issues": "https://github.com/DerManoMann/acache/issues",
        "source": "https://github.com/DerManoMann/acache.git"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": [
            "vendor/bin/php-cs-fixer fix src",
            "vendor/bin/php-cs-fixer fix tests"
        ]
    },
    "require": {
        "php": ">=7.1",
        "psr/cache": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "cache/integration-tests": "^0.16",
        "cache/cache": "^1.0",
        "php-coveralls/php-coveralls": "~2.1",
        "phpunit/php-code-coverage": "^6.1|^7.0.5",
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer":"^2.13",
        "doctrine/cache": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Radebatz\\ACache\\": "src/Radebatz/ACache"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
